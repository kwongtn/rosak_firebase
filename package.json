        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "lint-staged": "^15.0.3",
        "prettier": "^3.0.0",
        "typescript": ">=5.4.0 <5.6.0",
        "webpack-bundle-analyzer": "^4.10.2"
    }
