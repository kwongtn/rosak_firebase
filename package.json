{
    "name": "public",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "buildProd": "angular-build-info && ng build --configuration production && npm run sentry:sourcemaps",
        "buildStaging": "angular-build-info && ng build --configuration staging --stats-json --verbose && npm run sentry:sourcemaps",
        "watch": "ng build --watch --configuration development",
        "test": "ng test",
        "prepare": "husky",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org kwongtn --project javascript-angular ./dist && sentry-cli sourcemaps upload --org kwongtn --project javascript-angular ./dist"
    },
    "lint-staged": {
        "*.{js,ts}": [
            "eslint --fix"
        ],
        "*.{json,css,scss,html,md}": [
            "prettier --write"
        ]
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^17.3.0",
        "@angular/common": "^17.3.0",
        "@angular/compiler": "^17.3.0",
        "@angular/core": "^17.3.0",
        "@angular/fire": "^17.0.1",
        "@angular/forms": "^17.3.0",
        "@angular/platform-browser": "^17.3.0",
        "@angular/platform-browser-dynamic": "^17.3.0",
        "@angular/router": "^17.3.0",
        "@antv/g2": "^5.1.6",
        "@antv/g2plot": "^2.4.23",
        "@antv/l7": "^2.21.10",
        "@antv/l7-maps": "^2.21.10",
        "@antv/scale": "^0.3.11",
        "@apollo/client": "^3.8.4",
        "@devui-design/icons": "^1.3.0",
        "@sentry/angular-ivy": "^7.61.0",
        "@sentry/cli": "^2.23.1",
        "@sentry/replay": "^7.24.2",
        "@sentry/tracing": "^7.7.0",
        "@supercharge/promise-pool": "^3.0.0",
        "angular-build-info": "^2.0.1",
        "apollo-angular": "^6.0.0",
        "csvtojson": "^2.0.10",
        "emoji-toolkit": "^8.0.0",
        "firebase": "^10.0.0",
        "first-input-delay": "^0.1.3",
        "graphql": "^16.8.1",
        "gtfs": "^4.10.4",
        "gtfs-realtime-bindings": "^1.1.1",
        "jszip": "^3.10.1",
        "marked": "^9.0.0",
        "mermaid": "^10.6.0",
        "ng-devui": "^17.0.0",
        "ng-in-viewport": "^16.0.0",
        "ng-recaptcha": "^13.0.0",
        "ng-zorro-antd": "^17.3.0",
        "ngx-markdown": "^17.0.0",
        "rxjs": "^7.4.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.14.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "^17.0.0",
        "@angular-devkit/build-angular": "^17.3.0",
        "@angular/cli": "^17.3.0",
        "@angular/compiler-cli": "^17.3.0",
        "@codecov/webpack-plugin": "^0.0.1-beta.10",
        "@sentry/webpack-plugin": "^2.5.0",
        "@types/geojson": "^7946.0.14",
        "@types/jasmine": "~5.1.4",
        "@types/mapbox-gl": "^3.1.0",
        "@types/node": "^20.0.0",
        "@types/react-dom": "^18.0.9",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-json": "^3.1.0",
        "gtfs-types": "^4.0.0",
        "husky": "^9.0.0",
        "jasmine-core": "~5.1.2",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "^3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "^2.0.0",
        "lint-staged": "^15.0.3",
        "prettier": "^3.0.0",
        "typescript": "^5.4.2",
        "webpack-bundle-analyzer": "^4.10.2"
    }
}
